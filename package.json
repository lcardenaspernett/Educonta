{
  "name": "educonta",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "install-complete": "node scripts/install-complete-system.js",
    "migrate-students": "node scripts/migrate-students.js",
    "postinstall": "npx prisma generate",
    "build": "npx prisma db push && npx prisma generate",
    "deploy": "npx prisma db push && npx prisma generate && node server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:setup": "npx prisma migrate deploy && npx prisma generate && npx prisma db seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-server": "node test-server.js",
    "diagnose": "node scripts/diagnose-server.js",
    "lint": "echo \"Linting not configured\" && exit 0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.22.0",
    "xlsx": "^0.18.5"
  }
}
